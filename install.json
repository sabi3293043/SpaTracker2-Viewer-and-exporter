{
  "run": [
    {
      "method": "shell.run",
      "params": {
        "message": [
          "git clone https://github.com/henry123-boy/SpaTrackerV2 app"
        ]
      }
    },
    {
      "method": "shell.run",
      "params": {
        "venv": "venv",
        "path": "app",
        "chain": true,
        "input": null,
        "message": [
          "uv pip install -r requirements.txt"
        ]
      }
    },
    {
      "method": "script.start",
      "params": {
        "uri": "torch.js",
        "params": {
          "path": "app",
          "venv": "venv",
          "xformers": true,
          "triton": true,
          "sageattention": true
        }
      }
    }
  ]
}